{
  "name": "alpinedigital/craft",
  "require": {
    "alpinedigital/craft-cookie-notice": "^1.0",
    "craftcms/cms": "4.4.13",
    "craftcms/feed-me": "5.1.3.1",
    "craftcms/postmark": "^3.0",
    "craftcms/redactor": "3.0.4",
    "enupal/translate": "^3.0",
    "hybridinteractive/craft-position-fieldtype": "4.0.0",
    "nystudio107/craft-imageoptimize": "4.0.5",
    "nystudio107/craft-seomatic": "4.0.27",
    "statikbe/craft-config-values": "2.0.2",
    "statikbe/craft-cta-field": "^2.1",
    "statikbe/craft-deepl": "^1.0",
    "statikbe/craft-video-parser": "^2.1",
    "studioespresso/craft-dumper": "^3.0",
    "studioespresso/craft-navigate": "3.1.2",
    "vaersaagod/matrixmate": "2.1.3",
    "verbb/expanded-singles": "2.0.1",
    "verbb/formie": "2.0.30",
    "verbb/hyper": "1.1.7",
    "verbb/super-table": "3.0.9",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\alpinedigital\\": "modules/alpinedigital/src"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
